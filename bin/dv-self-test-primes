#!/bin/bash
source $(dv-path lib/common.sh) # -*-mode: sh-mode -*-

cd $(dv-cell-create primes)
echo 'Program to print the primes between its two arguments, one per line, runnable as node ./src/primes.js' > docs/user-request.md
